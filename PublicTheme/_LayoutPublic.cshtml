        (function () {
            "use strict";
            aic.ng.app.module.controller('layoutPubController', LayoutPubController);

            LayoutPubController.$inject = ['$scope', '$baseController', "$layoutPubService", "toastr", "$timeout", "$userService"];

            function LayoutPubController(
                $scope
                , $baseController
                , $layoutPubService
                , $toastr
                , $timeout
                , $userService) {

                //  controllerAs with vm syntax: https://github.com/johnpapa/angular-styleguide#style-y032
                var vm = this;//this points to a new {}

                vm.$layoutPubService = $layoutPubService;
                vm.$userService = $userService;
                vm.$scope = $scope;
                vm.profileLink = "/ProfilePageNg/" + aic.page.personId + "/Public"

                //  bindable members (functions) always go up top while the "meat" of the functions go below: https://github.com/johnpapa/angular-styleguide#style-y033

                //whatever functions you want to define
                vm.logout = _logout;
                vm.onLogoutSuccess = _onLogoutSuccess;
                //vm.poll = _poll;
                vm.unreadMessageCount = null;
                vm.onPollSuccess = _onPollSuccess;

                //-- this line to simulate inheritance
                $baseController.merge(vm, $baseController);

                //this is a wrapper for our small dependency on $scope
                vm.notify = vm.$layoutPubService.getNotifier($scope);

                //this is like the aic.startUp function
                render();

                function render() {
                    if (aic.page.guid != null && aic.page.personId != 0) {
                        vm.$userService.unreadMessages(vm.onPollSuccess);

                    }
                }


                function _logout() {
                    vm.$layoutPubService.logout(vm.onLogoutSuccess);
                }

                function _onLogoutSuccess() {
                    $toastr.success("You have successfully logged out.", { timeOut: 1000 });
                    self.location = '/Home/Index';
                }
                //function _poll() {
                //    $timeout(function () {
                //        vm.$userService.unreadMessages(vm.onPollSuccess);
                //        vm.poll();
                //    }, 5000);
                //};
                function _onPollSuccess(data) {
                    if (data.item.unreadMessageNumber != 0) {
                        vm.unreadMessageCount = data.item.unreadMessageNumber;
                        $timeout(function () {
                            vm.$userService.unreadMessages(vm.onPollSuccess);
                        }, 5000);
                    }
                    else {
                        vm.unreadMessageCount = null;
                        $timeout(function () {
                            vm.$userService.unreadMessages(vm.onPollSuccess);
                        }, 5000);
                    }
                }

            }
        })();
